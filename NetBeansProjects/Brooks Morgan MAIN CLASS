    private void btntanhActionPerformed(java.awt.event.ActionEvent evt) {                                        
            double ops = Double.parseDouble(String.valueOf(textbox.getText()));
      ops=Math.tanh(ops);
      textbox. setText(String.valueOf(ops));
    }                                       

